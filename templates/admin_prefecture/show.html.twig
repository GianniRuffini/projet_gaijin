{% extends 'base-admin.html.twig' %}

{% block title %}Prefecture{% endblock %}

{% block body %}
    <h1>Prefecture</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ prefecture.id }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ prefecture.titre }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ prefecture.description }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('admin_prefecture_index') }}">back to list</a>

    <a href="{{ path('admin_prefecture_edit', {'id': prefecture.id}) }}">edit</a>

    {{ include('admin_prefecture/_delete_form.html.twig') }}
