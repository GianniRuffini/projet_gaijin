{% extends 'base-admin.html.twig' %}

{% block title %}CategoryFaq index{% endblock %}

{% block body %}
    <h1>CategoryFaq index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Titre</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for category_faq in category_faqs %}
            <tr>
                <td>{{ category_faq.id }}</td>
                <td>{{ category_faq.titre }}</td>
                <td>
                    <a href="{{ path('admin_category_faq_show', {'id': category_faq.id}) }}">show</a>
                    <a href="{{ path('admin_category_faq_edit', {'id': category_faq.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('admin_category_faq_new') }}">Create new</a>
{% endblock %}
